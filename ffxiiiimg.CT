          <Description>"Faster TP Gain (In Battle)"</Description>
          <LastState/>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript Async="1">[ENABLE]
aobscanmodule(FastTPGain,ffxiiiimg.exe,89 48 6C 8B 4D FC)
alloc(newmem,$1000)
label(code,Exit,TPCap)
registersymbol(FastTPGain)

newmem:
imul ecx, ecx, 02
cmp ecx, #2100
ja TPCap

code:
mov [eax+6C], ecx
mov ecx, [ebp-04]
jmp Exit

TPCap:
mov ecx, #2100
jmp code

FastTPGain:
jmp newmem
nop

Exit:

[DISABLE]
FastTPGain:
db 89 48 6C 8B 4D FC
unregistersymbol(FastTPGain)
dealloc(newmem)
</AssemblerScript>
